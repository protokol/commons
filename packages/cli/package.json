{
	"name": "@protokol/cli",
	"version": "1.0.0-beta.37",
	"description": "A simple command line utility to create and broadcast NFT transactions.",
	"license": "MIT",
	"homepage": "https://docs.protokol.com/nft/",
	"bugs": {
		"url": "https://github.com/protokol/commons/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/protokol/commons.git"
	},
	"keywords": [
		"protokol",
		"ark",
		"core",
		"sdk",
		"nft",
		"blockchain"
	],
	"contributors": [
		"Žan Kovač <zan@protokol.com",
		"Amadej Pevec <amadej@protokol.com>",
		"Kristjan Košič <kristjan@protokol.com>",
		"Joshua Noack <joshua@ark.io>",
		"Sebastijan Kuzner <sebastijan@ark.io>"
	],
	"files": [
		"/dist",
		"/bin",
		"/oclif.manifest.json"
	],
	"bin": "./bin/run",
	"main": "dist/index",
	"types": "dist/index",
	"scripts": {
		"build": "yarn clean && tsc",
		"build:watch": "yarn build -w",
		"clean": "rimraf .coverage dist tmp",
		"postpack": "rm -f oclif.manifest.json",
		"publish:beta": "yarn build && yarn npm publish --tag beta --access public --tolerate-republish"
	},
	"dependencies": {
		"@arkecosystem/core-kernel": "3.0.0-next.14",
		"@arkecosystem/core-magistrate-crypto": "3.0.0-next.14",
		"@arkecosystem/crypto": "3.0.0-next.14",
		"@oclif/command": "^1",
		"@oclif/errors": "^1.3.4",
		"@oclif/plugin-help": "^3",
		"@protokol/guardian-crypto": "1.0.0-beta.35",
		"@protokol/nft-base-crypto": "1.0.0-beta.52",
		"@protokol/nft-exchange-crypto": "1.0.0-beta.52",
		"fs-extra": "^9.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.8.0",
		"@types/eslint": "^7.2.5",
		"@types/eslint-plugin-prettier": "^3.1.0",
		"@types/hapi__boom": "^7.4.1",
		"@types/hapi__joi": "^17.1.6",
		"@types/is-url-superb": "3.0.0",
		"@types/jest": "^26.0.15",
		"@types/lodash.orderby": "4.6.6",
		"@types/node": "^14.14.8",
		"@types/prettier": "^2.1.5",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "7.3.4",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.8.1",
		"@typescript-eslint/parser": "^4.8.1",
		"codecov": "^3.8.1",
		"cross-env": "^7.0.2",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^7.0.0",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-simple-import-sort": "^6.0.0",
		"got": "^11.8.0",
		"jest": "^26.6.3",
		"jest-extended": "^0.11.5",
		"jest-localstorage-mock": "^2.4.3",
		"nock": "^13.0.5",
		"npm-check-updates": "^10.2.1",
		"prettier": "^2.1.2",
		"rimraf": "^3.0.2",
		"ts-jest": "~26.4.4",
		"typedoc": "~0.19.2",
		"typescript": "~4.1.0"
	},
	"oclif": {
		"commands": "./dist/commands",
		"bin": "proto-cli",
		"topics": {
			"send": {
				"description": "Send transactions of various types"
			}
		},
		"plugins": [
			"@oclif/plugin-help"
		]
	}
}
